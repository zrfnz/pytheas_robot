<launch>
    <param name="robot_description" command="$(find xacro)/xacro '$(find robot_description)/urdf/homebot.urdf.xacro'" />
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher">
    	<param name="publish_frequency" type="double" value="50.0" />
    	<param name="tf_prefix" value=""/>
    </node>
    <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui"/>
    <!--
    <param name="use_gui" value="$(arg gui)" />
     -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find robot_description)/rviz/home_robot_preview.rviz" required="true" />
</launch>
